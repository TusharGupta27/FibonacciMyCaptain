#Assigning Elements to Different lists

n=int(input("Enter the number of elements for list 1"))
for i in range(0,n):
    element=int(input())
    list1.append(element)
print("Elements in list 1: ")
print(list1)
list2=[]
list2=list1
print("Elements in list 2: ")
print(list2)

#Accessing Elements From tuple

tuple1=(1,2,3,"Hello","Captain")
for i in range(0,len(tuple1)):
    print(tuple1[i])
    
#Deleting Different Dictionary elements

dict1={'Name':'Tushar','Id':123}
dict2={'Course':'Artificial Intelligence',"Offered by":'MyCaptain'}
print("Before Deletion")
print(dict1,dict2)
dict1.pop('Id')
dict2.pop('Course')
print("After Deletion")
print(dict1,dict2)
